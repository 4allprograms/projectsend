{
	"name": "projectsend/projectsend",
	"description": "ProjectSend is an open source, clients-oriented, private file sharing web application.",
	"type": "project",
	"license": "GPL-2.0-or-later",
	"homepage": "https://www.projectsend.org/",
	"support": {
			"email": "contact@projectsend.org",
			"issues": "https://github.com/projectsend/ProjectSend/issues",
			"source": "https://github.com/projectsend/ProjectSend/"
	},
	"config": {
		"preferred-install": "dist"
	},
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"ProjectSend\\": "app/classes/"
		},
		"files": [
			"app/includes/database.php",
			"app/includes/session.information.php",
			"app/includes/session.keep.alive.php",
			"app/includes/core.update.legacy.php",
			"app/includes/core.update.php",
			"app/includes/core.update.silent.php",
			"app/includes/functions.categories.php",
			"app/includes/functions.forms.php",
			"app/includes/functions.php",
			"app/includes/functions.templates.php",
			"app/includes/language.load.php",
			"app/includes/language.locales.names.php",
			"app/includes/options.get.php",
			"app/includes/text.strings.php",
			"app/includes/updates.functions.php",
			"app/includes/updates.messages.php",
			"app/includes/session.check.php",
			"app/includes/security/xsrf.php"
		]
	},
	"require": {
		"phpmailer/phpmailer": "^6.0",
		"symfony/polyfill-mbstring": "^1.8",
		"league/oauth2-google": "^2.2",
		"moxiecode/plupload": "3.1.*",
		"bainternet/php-hooks": "^1.0",
		"claviska/simpleimage": "^3.3",
		"paragonie/random_compat": "^2.0"
	}
}
